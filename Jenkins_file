pipeline{
  agent any
  stages{
      stage("Git Checkout"){
          steps{
          
          
              git credentialsId: 'MyGitHub', url: 'https://github.com/ashisharyaa/mahalogin.git'
          
            }
            
            stage("Maven Build"){
              steps{
                
                sh "mvn clean package"
                
            }
  
  
        }
  
    }
}
